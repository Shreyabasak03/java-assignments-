// Write a java program to print Factorial of a given number. Use any loop.

import java.util.*;
public class fact{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=5;
        int factorial=1;
        for(int i=1;i<=n;i++){
            factorial=factorial*i;
        }
        System.out.println(factorial);
    }
    
}
