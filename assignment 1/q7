import java.util.*;
class Seq
{
	void sq(int num){
		int i, a=1;
		for(i=1;i<=num;i++){
		a*=i;
		System.out.print((2*i)+", "+a+", ");
		
		}
	}
}

class Q7{

	public static void main(String a[]){
		int n;
		Scanner sc= new Scanner(System.in);
		System.out.print("Enter Sequence limit : ");
		n=sc.nextInt();
		Seq obj=new Seq();
		obj.sq(n);
	}
}
