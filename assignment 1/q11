//11. Java Program to Calculate the Power of a Number.

import java.util.*;
public class sum {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        // double n;
        System.out.println("enter value:");
        double n=sc.nextDouble();
        // int p;
        System.out.println("enter value:");
        int p=sc.nextInt();
        double r=1;
        for(int i=1;i<=p;i++){
            r=r*n;
        }
        System.out.println(r);
    }
}

