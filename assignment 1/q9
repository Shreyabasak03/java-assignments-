//9. Write a java program to print sum of n terms in the series 1/1! +1/2+1/3......

import java.util.*;
public class fact{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=5;
        int factorial=1;
        double s=0.0;
        for(int i=1;i<=n;i++){
            factorial=factorial*i;
            s=s+1.0/factorial;
        }
        System.out.println(s);
    }
    
}
