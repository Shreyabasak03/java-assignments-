//Write a java program to print primes from 1 to 1000.
import java.util.*;
public class PrimeNumbers{
    public static void main(String args[]){
        Scanner scn=new Scanner(System.in);
    for(int n=1;n<=1000;n++){
        int count=0;
    for(int a=2;a*a<=n;a++){
        if(n%a==0){
            count++;
            break;
        }
    }
    
if(count==0){
System.out.println(n);
    }
}

} 
}
